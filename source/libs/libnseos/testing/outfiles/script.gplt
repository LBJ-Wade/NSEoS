set terminal postscript eps size 8.0,5.0 enhanced color \
font 'Helvetica,25' linewidth 2
set log x
set format x '%.2g'
set xlabel 'n_B [fm^{-3}]'

set output 'aa.eps'
set log y
set format y '%.2g' 
set key top left
set ylabel 'A'
plot [0.0003:][] 'n2.out' u 1:2 w l lw 4 t 'CLDM ELFc N=2', \
'n3.out' u 1:2 w l lw 4 t 'CLDM ELFc N=3', \
'n4.out' u 1:2 w l lw 4 t 'CLDM ELFc N=4', \
'sly4.out' u 1:2 w l lw 4 t 'CLDM SLy4'
unset log y
unset format y

set output 'ii.eps'
set key top left
set ylabel 'I'
plot [0.0003:][] 'n2.out' u 1:3 w l lw 4 t 'CLDM ELFc N=2', \
'n3.out' u 1:3 w l lw 4 t 'CLDM ELFc N=3', \
'n4.out' u 1:3 w l lw 4 t 'CLDM ELFc N=4', \
'sly4.out' u 1:3 w l lw 4 t 'CLDM SLy4'

set output 'n0.eps'
set key bottom left
set ylabel 'n_0 [fm^{-3}]'
plot [0.0003:][] 'n2.out' u 1:4 w l lw 4 t 'CLDM ELFc N=2', \
'n3.out' u 1:4 w l lw 4 t 'CLDM ELFc N=3', \
'n4.out' u 1:4 w l lw 4 t 'CLDM ELFc N=4', \
'sly4.out' u 1:4 w l lw 4 t 'CLDM SLy4'

set output 'ng.eps'
set key bottom right
set ylabel 'n_g/n_B'
plot [0.0003:][] 'n2.out' u 1:($5/$1) w l lw 4 t 'CLDM ELFc N=2', \
'n3.out' u 1:($5/$1) w l lw 4 t 'CLDM ELFc N=3', \
'n4.out' u 1:($5/$1) w l lw 4 t 'CLDM ELFc N=4', \
'sly4.out' u 1:($5/$1) w l lw 4 t 'CLDM SLy4'
