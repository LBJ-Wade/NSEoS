set terminal postscript eps size 8.0,6.0 enhanced color \
font 'Helvetica,25' linewidth 2
set xlabel "I"
set ylabel "E_s/A^{2/3}"

set output 'surfenvac.eps'
set title "A = 150"
plot [-0.6:0.6][:] 'surf1.out' u 1:2 w l lw 4 t 'CLDM meta SLy4', \
'surf1.out' u 1:3 w l lw 4 t 'ETF meta SLy4', \
'surf2.out' u 1:4 w l lw 4 t "LS meta SkI'"

set output 'surfenmed.eps'
set key bottom left
set title "A = 300; w/ BBP correction"
plot [0.2:0.9][:] 'inmed1.out' u 1:2 w l lw 4 lc 1 t 'CLDM meta SLy4; n_g=0', \
'inmed1.out' u 1:5 w l lw 4 lc 1 dashtype 2 t 'CLDM meta SLy4; n_g=0.4n_0', \
'inmed1.out' u 1:3 w l lw 4 lc 2 t 'ETF meta SLy4; n_g=0', \
'inmed1.out' u 1:6 w l lw 4 lc 2 dashtype 2 t 'ETF meta SLy4; n_g=0.4n_0', \
'inmed2.out' u 1:4 w l lw 4 lc 3 t "LS meta SkI'; n_g=0", \
'inmed2.out' u 1:7 w l lw 4 lc 3 dashtype 2 t "LS meta SkI'; n_g=0.4n_0"

