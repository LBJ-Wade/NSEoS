set terminal postscript eps size 8.0,5.0 enhanced color \
font 'Helvetica,25' linewidth 2
set log x
set format x '%.2g'
set xlabel 'n_B [fm^{-3}]'

set output 'aabbp.eps'
set log y
set format y '%.2g' 
set key top left
set ylabel 'A'
plot [0.0003:][] 'icrust_eos_sly4.out' u 1:2 w l lw 4 t 'CLDM SLy4 w/o BBP corr', \
'icrust_eos_sly4_wbbpcorr.out' u 1:2 w l lw 4 t 'CLDM SLy4 w/ BBP corr'
unset log y
unset format y

set output 'iibbp.eps'
set key top left
set ylabel 'I'
plot [0.0003:][] 'icrust_eos_sly4.out' u 1:3 w l lw 4 t 'CLDM SLy4 w/o BBP corr', \
'icrust_eos_sly4_wbbpcorr.out' u 1:3 w l lw 4 t 'CLDM SLy4 w/ BBP corr'

set output 'n0bbp.eps'
set key bottom left
set ylabel 'n_0 [fm^{-3}]'
plot [0.0003:][] 'icrust_eos_sly4.out' u 1:4 w l lw 4 t 'CLDM SLy4 w/o BBP corr', \
'icrust_eos_sly4_wbbpcorr.out' u 1:4 w l lw 4 t 'CLDM SLy4 w/ BBP corr'

set output 'ngbbp.eps'
set key bottom right
set ylabel 'n_g/n_B'
plot [0.0003:][] 'icrust_eos_sly4.out' u 1:($5/$1) w l lw 4 t 'CLDM SLy4 w/o BBP corr', \
'icrust_eos_sly4_wbbpcorr.out' u 1:($5/$1) w l lw 4 t 'CLDM SLy4 w/ BBP corr'

set output 'enbbp.eps'
unset log x
unset format x
set key top left
set ylabel '{/Symbol e}_{WS} - amu n_B [MeV/fm^{3}]'
plot [0:0.055][0:0.8] 'icrust_eos_sly4.out' u 1:6 w l lw 4 t 'CLDM SLy4 w/o BBP corr', \
'icrust_eos_sly4_wbbpcorr.out' u 1:6 w l lw 4 t 'CLDM SLy4 w/ BBP corr'

set output 'pressbbp.eps'
set log
set format '%.2g'
set ylabel 'p_{WS} [MeV/fm^{3}]'
plot [1e-7:0.2][1e-8:1] 'icrust_eos_sly4.out' u 1:7 w l lw 4 t 'CLDM SLy4 w/o BBP corr', \
'icrust_eos_sly4_wbbpcorr.out' u 1:7 w l lw 4 t 'CLDM SLy4 w/ BBP corr'
